{
    "ignorePatterns": [],
    "settings": {},
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "jsx-quotes": "error",
        "quotes": "error",
        "semi": "error",
        "prettier/prettier": [
            "warn",
            {
                "singleQuote": false,
                "semi": true,
                "trailingComma": "all"
            }
        ]
    }
}
